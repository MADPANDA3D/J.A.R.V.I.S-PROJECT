{
  "name": "jarvis-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "./scripts/build-analysis.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "NODE_OPTIONS='--max-old-space-size=51200' vitest run --pool=threads --poolOptions.threads.maxThreads=1 --no-coverage --testTimeout=15000 --teardownTimeout=10000",
    "test:run:ci": "NODE_OPTIONS='--max-old-space-size=51200' vitest run --pool=threads --poolOptions.threads.maxThreads=1 --no-coverage --reporter=verbose --testTimeout=15000 --teardownTimeout=10000",
    "test:ci": "node scripts/force-exit-after-tests.cjs",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@datadog/browser-rum": "^6.16.0",
    "@hookform/resolvers": "^5.1.1",
    "@octokit/rest": "^22.0.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/react": "^10.5.0",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "logrocket": "^10.1.0",
    "lucide-react": "^0.525.0",
    "node-fetch": "^2.7.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.0",
    "react-router-dom": "^7.7.0",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "xml2js": "^0.6.2",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^6.0.3",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "caniuse-lite": "^1.0.30001734",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "yarn": "^1.22.22"
  }
}
